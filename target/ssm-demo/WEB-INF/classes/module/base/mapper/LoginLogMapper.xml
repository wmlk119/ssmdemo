<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.archer.ssm.module.base.mapper.LoginLogMapper" >

    <!-- 新增 -->
    <insert id="add" parameterType="com.archer.ssm.module.base.pojo.LoginLog">
        INSERT INTO login_log(logId,userId,loginName,userName,loginIp,address,point,createTime)
        VALUES (#{logId},#{userId},#{loginName},#{userName},#{loginIp},#{address},#{point},#{createTime})
    </insert>



</mapper>